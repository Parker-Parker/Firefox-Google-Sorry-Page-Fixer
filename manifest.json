{
    "manifest_version": 2,
    "name": "Google Unusual Traffic Page Fixer",
    "version": "0.5",
    "description": "Easily manually navigate to intended page after hitting Google 'unusual traffic' gate",
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": ["*://*.google.com/sorry/*"],
            "js": ["unsorry.js"]
        }
    ],
    "browser_action": {
        "default_title": "Google Sorry Fixer"
    },
    
    "browser_specific_settings": {
        "gecko": {
          "id": "todo@Parker-Parker.Github.io"
        }
    }, 
    
    "permissions": [ 
        
    ]
    
}